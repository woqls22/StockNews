<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="stock.stockproject.dao.NewsDAO"><!--namespace를 통해 NewsDAO 연결합니다. -->
    <select id="getTop20"  parameterType="stock.stockproject.dto.NewsDTO" resultType="stock.stockproject.dto.NewsDTO"><!-- getTop20()메서드를 호출하면 실행할 SELECT문,
						NewsDTO 객체를 파라미터로 받아, NewsDTO객체를 반환-->
        SELECT * FROM News
    </select>
</mapper>
